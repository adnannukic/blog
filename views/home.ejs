<%- include("partials/header") %> 
<h1>Home</h1>
<br>
<p><%= homePageContent %></p>
<% postsList.forEach(function (post) { %>
    <h1><%= post.title %></h1>
    <br>
    <p>
        <%= (post.intro + post.section1Title + post.section1 + post.section2Title + post.section2 + post.section3Title + post.section3 + post.conclusionTitle + post.conclusion).substring(0,100) + " ..." %>
        <a href="/post/<%= post.title.replace(/\s/g, '-') %>" >Read More</a>
    </p>
<% }); %> 

<%- include("partials/footer") %> 
