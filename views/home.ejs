<%- include("partials/header") %> 
<h1>Home</h1>

<p><%= homePageContent %></p>
<% postsList.forEach(function (post) { %>
    <h1><%= post.title %></h1>

    <p>
        <%= post.body.substring(0,100) + " ..." %>
        <a href="/post/<%= post.title.toLowerCase().replace(/\s/g, '-') %>" >Read More</a>
    </p>
<% }); %> 

<%- include("partials/footer") %> 
